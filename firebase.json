{
  "hosting": {
    "site": "shopping-hunter-api",
    "public": "public",
    "rewrites": [
      {
          "source": "**",
          "function": "app"
      }
  ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ]
}
